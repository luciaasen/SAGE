    "def F(m):\n",
    "    A = 545140134\n",
    "    B = 13591409\n",
    "    C = 640320\n",
    "    return ((-1)^m*(factorial(6*m))*(A*m+B))/(factorial(3*m)*(factorial(m)^3)*(C^(3*m)))\n",
    "\n",
    "for i in srange (1, 11):\n",
    "    show(F(i).n())"
   ]
